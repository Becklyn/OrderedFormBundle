{
    "name": "becklyn/ordered-form-bundle",
    "type": "symfony-bundle",
    "description": "This bundle adds functionality for ordering form elements",
    "homepage": "https://github.com/Becklyn/OrderedFormBundle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/config": "^4.0 || ^5.0",
        "symfony/dependency-injection": "^4.0 || ^5.0",
        "symfony/form": "^4.0 || ^5.0",
        "symfony/http-kernel": "^4.0 || ^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\OrderedFormBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\OrderedFormBundle\\": "tests/"
        }
    }
}
