{
    "name": "becklyn/ordered-form-bundle",
    "description": "This bundle adds functionality for ordering form elements",
    "license": "BSD-3-Clause",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "http://becklyn.com/"
        }
    ],
    "homepage": "https://github.com/Becklyn/OrderedFormBundle",
    "require": {
        "php": ">=7.4",
        "symfony/config": "^4.4 || ^v5.4.3 || ^v6.0.3",
        "symfony/dependency-injection": "^4.4 || ^v5.4.3 || ^v6.0.3",
        "symfony/form": "^4.4 || ^v5.4.3 || ^v6.0.3",
        "symfony/http-kernel": "^4.4 || ^v5.4.4 || ^v6.0.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "symfony/phpunit-bridge": "^4.4 || ^v5.4.3 || ^v6.0.3"
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\OrderedFormBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\OrderedFormBundle\\": "tests/"
        }
    }
}
